{
	"author": "Handyfon",
	"keywords": [
		"layout",
		"dnd",
		"vtt",
		"foundry"
	],
	"name": "foundryvtt-simple-mobile",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "node-sass --watch src/scss -o css",
		"build": "gulp build && gulp link && node-sass src/scss -o dist & postcss dist/simple-mobile.css -u autoprefixer --no-map -d dist/",
		"package": "gulp package",
		"build:watch": "gulp watch",
		"clean": "gulp clean && gulp link --clean",
		"update": "npm install --save-dev gitlab:foundry-projects/foundry-pc/foundry-pc-types"
	},
	"version": "0.2.6",
	"devDependencies": {
		"archiver": "5.3.0",
		"autoprefixer": "^9.1.0",
		"chalk": "^4.1.0",
		"cssnano": "4.1.10",
		"foundry-pc-types": "gitlab:foundry-projects/foundry-pc/foundry-pc-types",
		"fs-extra": "^9.0.1",
		"gulp": "^4.0.2",
		"gulp-git": "^2.10.1",
		"gulp-less": "^4.0.1",
		"gulp-sass": "^4.1.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"node-sass": "^5.0.0",
		"json-stringify-pretty-compact": "^2.0.0",
		"postcss": "^8.2.8",
		"sass": "^1.32.8",
		"teq-tonic": "*",
		"typescript": "^3.9.7",
		"yargs": "^15.4.1"
	},
	"dependencies": {
		"postcss-cli": "^8.1.0",
		"postcss-cli-simple": "^3.1.0"
	}
}
